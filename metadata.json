{
  "name": "opentable/teamcity",
  "version": "1.0.1",
  "author": "opentable",
  "license": "MIT",
  "summary": "Module for managing TeamCity agents and servers",
  "source": "https://github.com/opentable/puppet-teamcity.git",
  "project_page": "https://github.com/opentable/puppet-teamcity",
  "issues_url": "https://github.com/opentable/puppet-teamcity/issues",
